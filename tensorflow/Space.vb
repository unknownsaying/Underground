
Class X1Y1Z1
Sub X1
    for ( i = 0; i <= -3; ++i)
        for( j = 0; i <= -3; ++j)
            for( k = 0; k <= -3; ++k)
        continue
End Sub
Sub Y1
    for( l = 0;-3 <= l && l >= 3; ++l || --l)
       for( m = 0;-3 <= m && m >= 3 ;++m || --m)
          for( n = 0 ;-3 <= n && n >= 3;++n || n)
        break
End Sub
Sub Z1
    for( o = 0;o >= 3;--o)
        for( p = 0; p >= 3;--p)
            for( q = 0; q >= 3;--q)
        continue
End Sub
End Class

